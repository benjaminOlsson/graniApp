  body
    div#headerDiv.noSpaces
      div#centerHeadContent
        a(href="/users/#{id}")
          div#headerTitle
            h1#headerTitleText TeamBuilder
        a(href="/users/#{id}/addGroup")#headerAddGroup.gray Add Group
        a(href="/logout")#logOut.gray Log Out
        a(href="/users/#{id}/calendar")#headerCalendar.gray Calendar
        a(href="/users/#{id}/addTeam")#headerAddTeam.gray Add Team
    div#mainArea
