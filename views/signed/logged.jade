    title= title
    link(rel="stylesheet" type="text/css" href="/styles/signed/loggedTemplate.css")
    link(rel="stylesheet" type="text/css" href="/styles/signed/userProfiles.css")
    link(rel="stylesheet" type="text/css" href="/styles/signed/groupAndTeam.css")
